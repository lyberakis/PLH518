create table teachers (
    id varchar(100) primary key,
    name varchar(50),
    surname varchar(50),
    username varchar(30),
    password varchar(30),
    email varchar(20)
);

create table students (
    id varchar(100) primary key,
    name varchar(50),
    surname varchar(50),
    fathername varchar(50),
    grade float,
    mobilenumber varchar(10),
    birthday date
);